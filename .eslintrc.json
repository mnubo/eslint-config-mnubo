{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "env": {
        "browser": true,
        "jasmine": true,
        "node": true
    },
    "globals": {
        "angular": true,
        "browser": true,
        "by": true,
        "element": true,
        "expect": true,
        "protractor": true,
        "spyOn": true,
        "xdescribe": true,
        "xit": true
    },
    "plugins": [
        "jasmine"
    ],
    "extends": [],
    "rules": {
        "arrow-parens": 2,
        "arrow-spacing": [2, { "before": true, "after": true }],
        "no-alert": 2,
        "no-array-constructor": 2,
        "no-bitwise": 2,
        "no-caller": 2,
        "no-catch-shadow": 2,
        "no-comma-dangle": 0,
        "no-cond-assign": 2,
        "no-console": 2,
        "no-const-assign": 2,
        "no-constant-condition": 2,
        "no-control-regex": 2,
        "no-debugger": 2,
        "no-delete-var": 2,
        "no-div-regex": 0,
        "no-dupe-keys": 2,
        "no-else-return": 0,
        "no-empty": 2,
        "no-empty-character-class": 2,
        "no-eq-null": 0,
        "no-eval": 2,
        "no-ex-assign": 2,
        "no-extend-native": 2,
        "no-extra-boolean-cast": 2,
        "no-extra-parens": 2,
        "no-extra-semi": 2,
        "no-fallthrough": 2,
        "no-floating-decimal": 0,
        "no-func-assign": 2,
        "no-global-strict": 0,
        "no-implicit-coercion": [2, {"string": true}],
        "no-implied-eval": 2,
        "no-invalid-regexp": 2,
        "no-invalid-this": 0,
        "no-iterator": 2,
        "no-label-var": 2,
        "no-labels": 2,
        "no-lone-blocks": 2,
        "no-lonely-if": 0,
        "no-loop-func": 2,
        "no-mixed-requires": [0, false],
        "no-multiple-empty-lines": [2, {"max": 1}],
        "no-multi-str": 2,
        "no-native-reassign": 2,
        "no-negated-in-lhs": 2,
        "no-nested-ternary": 0,
        "no-new": 2,
        "no-new-func": 2,
        "no-new-object": 2,
        "no-new-require": 0,
        "no-new-wrappers": 2,
        "no-obj-calls": 2,
        "no-octal": 2,
        "no-octal-escape": 2,
        "no-path-concat": 0,
        "no-plusplus": 0,
        "no-process-exit": 2,
        "no-proto": 2,
        "no-redeclare": 2,
        "no-regex-spaces": 2,
        "no-restricted-modules": 0,
        "no-return-assign": 2,
        "no-script-url": 2,
        "no-self-compare": 0,
        "no-sequences": 2,
        "no-shadow": 0,
        "no-shadow-restricted-names": 2,
        "no-spaced-func": 2,
        "no-sparse-arrays": 2,
        "no-sync": 0,
        "no-ternary": 0,
        "no-trailing-spaces": 2,
        "no-undef": 2,
        "no-undef-init": 2,
        "no-underscore-dangle": 0,
        "no-unreachable": 2,
        "no-unused-expressions": 2,
        "no-unused-vars": [2, {"vars": "all","args": "after-used"}],
        "no-use-before-define": 0,
        "no-warning-comments": [0, {"terms": ["todo", "fixme", "xxx"], "location": "start"}],
        "no-with": 2,
        "block-scoped-var": 0,
        "brace-style": 2,
        "camelcase": 2,
        "complexity": [0, 11],
        "consistent-return": 0,
        "consistent-this": [0, "that"],
        "curly": [2, "all"],
        "default-case": 0,
        "dot-notation": 2,
        "eqeqeq": 2,
        "func-names": 0,
        "func-style": [0, "declaration"],
        "guard-for-in": 0,
        "indent": [2, 4, {"SwitchCase": 1}],
        "key-spacing": [2, {"beforeColon": false, "afterColon": true}],
        "keyword-spacing": 2,
        "linebreak-style": [2, "unix"],
        "lines-around-comment": [0, {"afterBlockComment": true, "afterLineComment": true}],
        "max-depth": [0, 4],
        "max-len": [2, 100],
        "max-nested-callbacks": [0, 2],
        "max-params": [0, 3],
        "max-statements": [0, 10],
        "handle-callback-err": 0,
        "new-cap": 2,
        "new-parens": 2,
        "operator-linebreak": [2, "after"],
        "object-curly-spacing": [2, "always"],
        "one-var": [2, "never"],
        "padded-blocks": [2, {
            "blocks": "never",
            "classes": "never",
            "switches": "never"
        }],
        "quote-props": [2, "as-needed"],
        "quotes": [2, "single"],
        "radix": 0,
        "semi": 2,
        "semi-spacing": 2,
        "sort-vars": 0,
        "space-before-blocks": [2, "always"],
        "space-before-function-paren": [2, "never"],
        "space-in-brackets": [0, "never"],
        "space-in-parens": [2, "never"],
        "space-infix-ops": 2,
        "space-unary-ops": 0,
        "space-unary-word-ops": 0,
        "strict": [0, "function"],
        "use-isnan": 2,
        "valid-jsdoc": 0,
        "valid-typeof": 2,
        "wrap-iife": 0,
        "wrap-regex": 0,
        "yoda": [2, "never"],
        "jasmine/no-focused-tests": 2
    }
}
